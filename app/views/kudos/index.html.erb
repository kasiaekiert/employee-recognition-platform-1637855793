<h1>Kudos</h1>

<ul>
  <% kudos.each do |kudo| %>
    <li>
      <%= kudo.title %>
      <%= kudo.content %>
      <%= kudo.employee_id %>
      <%= kudo.created_at %>
      <%= link_to "Edit this Kudo", edit_kudo_path(kudo) %>
    </li>
  <% end %>

  <%= link_to "Create New Kudo", new_kudo_path %>
</ul>
